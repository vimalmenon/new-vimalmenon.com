{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "esnext",
        "target": "es6",
        "jsx": "react",
        "strict": true,
        "removeComments": false,
        "moduleResolution": "node",
        "preserveConstEnums": true,
        "esModuleInterop" : true,
        "baseUrl": "./",
        "types": [
            "@types/jest"
        ],
        "lib": [
            "ES6",
            "DOM",
            "ES2015.Promise",
            "webworker"
        ],
        "typeRoots" : ["assets/typings"],
        "paths": {
            "store": ["assets/store"],
            "model": ["assets/model"],
            "utility": ["assets/utility"],
            "component": ["assets/component"],
            "service": ["assets/service"],
            "action": ["assets/store/action"],
            "const" : ["assets/const"],
            "enums" :["assets/const/enums"],
            "image":["assets/image/"],
        }
    }
}